"\n","class MLStripper(HTMLParser):\n","    def __init__(self):\n","        self.reset()\n","        self.strict = False\n","        self.convert_charrefs= True\n","        self.fed = []\n","    def handle_data(self, d):\n","        self.fed.append(d)\n","    def get_data(self):\n","        return ''.join(self.fed)\n","\n","def strip_tags(html):\n","    s = MLStripper()\n","    s.feed(html)\n","    return s.get_data()\n","\n","def extract_features(path_to_data):\n","    \n","    content_list = [] \n","    published_list = [] \n","    title_list = []\n","    author_list = []\n","    domain_list = []\n","    tags_list = []\n","    url_list = []\n","\n","    with open(path_to_data, encoding='utf-8') as inp_json_file:\n","        for line in inp_json_file:\n","            json_data = read_json_line(line)\n","            content = json_data['content'].replace('\\n', ' ').replace('\\r', ' ')\n","            content_no_html_tags = strip_tags(content)\n","            content_list.append(content_no_html_tags)\n","            published = json_data['published']['$date']\n","            published_list.append(published) \n","            title = json_data['meta_tags']['title'].split('\\u2013')[0].strip() #'Medium Terms of Service – Medium Policy – Medium'\n","            title_list.append(title) \n","            author = json_data['meta_tags']['author'].strip()\n","            author_list.append(author) \n","            domain = json_data['domain']\n","            domain_list.append(domain)\n","            url = json_data['url']\n","            url_list.append(url)\n","            \n","            tags_str = []\n","            soup = BeautifulSoup(content, 'lxml')\n","            try:\n","                tag_block = soup.find('ul', class_='tags')\n","                tags = tag_block.find_all('a')\n","                for tag in tags:\n","                    tags_str.append(tag.text.translate({ord(' '):None, ord('-'):None}))\n","                tags = ' '.join(tags_str)\n","            except Exception:\n","                tags = 'None'\n","            tags_list.append(tags)    \n","    return content_list, published_list, title_list, author_list, domain_list, tags_list, url_list\n","\n","def extract_features_test(path_to_data):\n","    k = 0 \n","    with open(path_to_data, encoding='utf-8') as inp_json_file:\n","        for line in inp_json_file:\n","          k += 1         \n","    return k\n","\n"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"6VtJML4rZywB","colab_type":"code","outputId":"0172afb9-e466-4499-8129-4cc761addbb8","executionInfo":{"status":"ok","timestamp":1588153023478,"user_tz":-180,"elapsed":5592,"user":{"displayName":"Ардан Доржиев","photoUrl":"","userId":"12616677815845703401"}},"colab":{"base_uri":"https://localhost:8080/","height":34}},"source":["extract_features_test(os.path.join(PATH, 'train.json'))"],"execution_count":0,"outputs":[{"output_type":"execute_result","data":{"text/plain":["62313"]},"metadata":{"tags":[]},"execution_count":37}]},{"cell_type":"code","metadata":{"id":"ZZqEIkKBdKGH","colab_type":"code","outputId":"d9f4b566-7943-4ec5-cbf9-f27ceee2fa75","executionInfo":{"status":"ok","timestamp":1588153026851,"user_tz":-180,"elapsed":3361,"user":{"displayName":"Ардан Доржиев","photoUrl":"","userId":"12616677815845703401"}},"colab":{"base_uri":"https://localhost:8080/","height":34}},"source":["extract_features_test(os.path.join(PATH, 'test.json'))"],"execution_count":0,"outputs":[{"output_type":"execute_result","data":{"text/plain":["34645"]},"metadata":{"tags":[]},"execution_count":38}]},{"cell_type":"code","metadata":{"id":"0TH4_95PdXQl","colab_type":"code","colab":{}},"source":["content_list, published_list, title_list, author_list, domain_list, tags_list, url_list = extract_features(os.path.join(PATH, 'train.json'))\n","train = pd.DataFrame()\n","train['content'] = content_list\n","train['published'] = pd.to_datetime(published_list, format='%Y-%m-%dT%H:%M:%S.%fZ')\n","train['title'